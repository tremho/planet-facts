
#page main
title='Planet Facts'
app-menu
no-back

#content
<simple-label text="@planet.title"/>

<hr/>

<repeat-for-each subject="@planet.data"
                 fact="@planet.fact"
                 unit="@planet.unit">
    <labeled-value label="$item.name"
                   value="$item.%fact $%unit"/>
</repeat-for-each>

